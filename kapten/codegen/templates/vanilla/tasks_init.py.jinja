{% import "macros.jinja" as macros %}

{% for task_name in task_names -%}
{%- set task_obj = tasks_dict[task_name] -%}
{%- set script_name = macros.get_py_script_func_name(task_obj, task_name) -%}
{%- if "py_script" in task_obj -%}
from .{{ script_name }} import {{ task_name }}
{% endif -%}
{% endfor %}